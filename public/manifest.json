{
  "manifest_version": 2,
  "name": "ts2es",
  "version": "0.0.1",
  "description": "elasticSearch数据类型转换为typescript数据类型",
  "icons": {
    "16": "img/icon1.png",
    "48": "img/icon2.png",
    "128": "img/icon3.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon1.png",
    "default_title": "es-mapping-ts-interface"
  },
  "permissions": ["tabs", "webRequest", "webRequestBlocking", "storage"],
  "homepage_url": "https://github.com/YamadaAoi/es2ts"
}
